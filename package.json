{
  "name": "multi-entry-webpack-starter",
  "version": "1.2.1",
  "description": "a webpack starter for multi page applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfansimon/multi-entry-webpack-starter.git"
  },
  "author": "cfansimon@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cfansimon/multi-entry-webpack-starter/issues"
  },
  "devDependencies": {},
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chunk-manifest-webpack-plugin": "1.1.2",
    "copy-webpack-plugin": "4.0.1",
    "cors": "2.8.4",
    "cross-env": "5.0.5",
    "css-loader": "0.28.5",
    "eslint": "4.5.0",
    "eslint-config-airbnb-base": "11.3.2",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fix-moduleid-and-chunkid-plugin": "0.1.1",
