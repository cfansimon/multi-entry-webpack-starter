{
  "name": "multi-entry-webpack-starter",
  "version": "1.2.0",
  "description": "a webpack starter for multi page applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfansimon/multi-entry-webpack-starter.git"
  },
  "author": "cfansimon@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cfansimon/multi-entry-webpack-starter/issues"
  },
  "devDependencies": {
  },
  "dependencies": {
    "babel-cli": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.16.0",
    "chunk-manifest-webpack-plugin": "0.1.0",
    "cors": "2.8.1",
    "cross-env": "3.1.4",
    "css-loader": "0.23.1",
    "eslint": "3.3.0",
    "eslint-config-airbnb-base": "5.0.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.13.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "fix-moduleid-and-chunkid-plugin": "0.1.1",
    "copy-webpack-plugin": "4.0.1",
    "glob": "7.0.5",
