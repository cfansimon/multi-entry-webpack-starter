    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "path": "0.12.7",
    "serve-index": "1.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.15.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-notifier": "1.5.0",
    "winston": "2.2.0",
    "yargs": "4.8.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "compact": true,
    "comments": false
  },
  "bin": {
    "babel-node": "dist/bin/babel-node.js",
    "webpack": "dist/bin/webpack.js",
    "cross-env": "dist/bin/cross-env.js"
  },
  "scripts": {
    "start": "rm -rf dist/* && babel src --out-dir dist",
    "dev": "rm -rf dist/* && npm start -- --watch"
  }
}
